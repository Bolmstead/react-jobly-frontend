 path=\"/Companies\">\r\n                <Companies />\r\n            </Route>\r\n            <Route exact path=\"/Companies/:handle\" >\r\n                <Company />\r\n            </Route>\r\n            <Route exact path=\"/Jobs\">\r\n                <Jobs />\r\n            </Route>\r\n            <Route exact path=\"/Login\">\r\n                <LoginForm login={login}/>\r\n            </Route>\r\n            <Route exact path=\"/Signup\">\r\n                <RegisterForm signup={signup}/>\r\n            </Route>\r\n            <Route exact path=\"/Profile\">\r\n                <Profile />\r\n            </Route>\r\n            <Route><h1>Not Found!</h1></Route>\r\n        </Switch>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Routes;\r\n","C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Profile.js",["186","187"],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\NavBar.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Home.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\Api.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\CompanyComponents\\Companies.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\CompanyComponents\\Company.js",["188","189"],"import React, { useState, useEffect, useContext } from \"react\";\r\nimport {useParams} from 'react-router-dom';\r\nimport JoblyApi from \"../../Api\"\r\nimport LoadingSpinner from \"../Common/LoadingSpinner\";\r\nimport JobCard from \"../JobComponents/JobCard\"\r\nimport JobCardList from \"../JobComponents/JobCardList\"\r\nimport UserContext from '../Common/UserContext'\r\n\r\n\r\n\r\n\r\n\r\n/* Main component. Renders card lists for\r\n * playing cards and pokemon. */\r\nfunction Company() {\r\n  const {currentUser} = useContext(UserContext)\r\n  const {handle} = useParams();\r\n  const [company, setCompany] = useState([]);\r\n\r\n  useEffect(() => {\r\n    async function getComps() {\r\n      setCompany(await JoblyApi.getCompany(handle));\r\n    }\r\n    getComps();\r\n  }, []);\r\n\r\n  if (!currentUser) { return <h1>Access Unauthorized</h1>}\r\n  if (!company) return <LoadingSpinner />;\r\n  \r\n\r\n  return (\r\n    <div>\r\n        <h1>{company.name}</h1>\r\n        <p>{company.description}</p>\r\n        <h3>Position Openings:</h3>\r\n        <JobCardList jobs={company.jobs} />\r\n    </div>\r\n  );\r\n}\r\n\r\n\r\nexport default Company;\r\n","C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\CompanyComponents\\CompanyCard.js",["190","191","192"],"import React, { useState, useEffect } from \"react\";\r\nimport {Link} from 'react-router-dom';\r\nimport JoblyApi from \"../../Api\"\r\n\r\n/* Main component. Renders card lists for\r\n * playing cards and pokemon. */\r\nfunction CompanyCard(company) {\r\n\r\n  return (\r\n    <div>\r\n        <Link to={`/Companies/${company.handle}`} key={company.handle}>\r\n          <h4>{company.name}</h4>\r\n        </Link>\r\n        <p>{company.description}</p>\r\n\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default CompanyCard;\r\n","C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\JobComponents\\Jobs.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\JobComponents\\JobCard.js",["193","194","195","196"],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Common\\LoadingSpinner.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\JobComponents\\JobCardList.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Common\\SearchForm.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Signup.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Login.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Common\\TokenContext.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Common\\Alert.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\components\\Common\\UserContext.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly\\frontend\\src\\hooks\\useLocalStorage.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\index.js",[],["197","198"],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\App.js",["199","200"],"\nimport React, { useState, useEffect } from \"react\";\nimport Routes from './Routes'\nimport NavBar from './components/NavBar'\nimport { BrowserRouter } from \"react-router-dom\";\nimport JoblyApi from \"./Api\";\nimport jwt from \"jsonwebtoken\";\nimport UserContext from './components/Common/UserContext'\nimport useLocalStorage from \"./hooks/useLocalStorage\";\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport { Container } from 'reactstrap';\nimport './App.css';\n\nexport const TOKEN_STORAGE_ID = \"jobly-token\";\n\n/* Entire web app. Holds state for the currentUser, token, and jobs a user has applied for.\n  Grabs the current user that is logged in (if logged in). Holds login and signup functions. \n  Keeps track of which jobs a user has applied to. Renders Navbar and routes.\n   */\n\nfunction App() {\n  const [infoLoaded, setInfoLoaded] = useState(false);\n  const [currentUser, setCurrentUser] = useState(null);\n  const [token, setToken] = useLocalStorage(TOKEN_STORAGE_ID);\n  const [applicationIds, setApplicationIds] = useState(new Set([]));\n\n\n  useEffect(function loadUserInfo() {\n    console.debug(\"App useEffect loadUserInfo\", \"token=\", token);\n\n    async function getCurrentUser() {\n      if (token) {\n        try {\n          let { username } = jwt.decode(token);\n          // put the token on the Api class so it can use it to call the API.\n          JoblyApi.token = token;\n          let userResult = await JoblyApi.getCurrentUser(username);\n          console.log(\"userResult\",userResult)\n\n          setCurrentUser(userResult);\n          console.log(\"currentUser\",currentUser)\n\n          // setApplicationIds(new Set(currentUser.applications));\n        } catch (err) {\n          console.error(\"App loadUserInfo: problem loading\", err);\n          setCurrentUser(null);\n        }\n      }\n      setInfoLoaded(true);\n    }\n\n    // set infoLoaded to false while async getCurrentUser runs; once the\n    // data is fetched (or even if an error happens!), this will be set back\n    // to false to control the spinner.\n    setInfoLoaded(false);\n    getCurrentUser();\n    console.log(\"token\",token)\n\n  }, [token]);\n  console.log(\"currentUser\",currentUser)\n\n  async function login(loginData) {\n    try {\n      let token = await JoblyApi.login(loginData);\n      setToken(token);\n      console.log(\"login success\")\n      return { success: true };\n    } catch (errors) {\n      console.error(\"login failed\", errors);\n      return { success: false, errors };\n    }\n  }\n\n  function logout() {\n    setCurrentUser(null);\n    setToken(null);\n  }\n\n  async function signup(data) {\n    try {\n      let token = await JoblyApi.signup(data);\n      setToken(token);\n      console.log(\"signup success\")\n      return { success: true };\n    } catch (errors) {\n      console.error(\"signup failed\", errors);\n      return { success: false, errors };\n    }\n  }\n  /** Checks if a job has been applied for. */\n  function hasAppliedToJob(id) {\n    return applicationIds.has(id);\n  }\n\n  /** Apply to a job: make API call and update set of application IDs. */\n  function applyToJob(id) {\n    if (hasAppliedToJob(id)) return;\n    JoblyApi.applyToJob(currentUser.username, id);\n    setApplicationIds(new Set([...applicationIds, id]));\n  }\n\n\n  return (\n    <div className=\"App\">\n      <BrowserRouter>\n      <UserContext.Provider value={{ currentUser, setCurrentUser, hasAppliedToJob, applyToJob}}>\n        <NavBar logout={logout} />\n        <Container>\n        <Routes login={login} signup={signup}/>\n        </Container>\n        </UserContext.Provider>\n      </BrowserRouter>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\Routes.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\Api.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\components\\Common\\UserContext.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\components\\NavBar.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current VS Code Projects\\react-jobly-final\\frontend\\src\\components\\Common\\LoadingSpinner.js",[],"C:\\Users\\olms2\\OneDrive\\Desktop\\Coding\\Springboard\\Current